<% provide :title, t(".title") %>

<h2 class="title"><%= t ".subcat" %></h2>

<div class="container">
  <table class="table table-hover table-condensed">
    <thead>
      <tr>
        <th class="th1"><%= t ".name" %></th>
        <th class="th2"><%= t ".func" %></th>
      </tr>
    </thead>
    <tbody>
      <%= render "sub_category" %>
    </tbody>
    <tfoot>
      <%= will_paginate @subcategory %>
    </tfoot>
  </table>
</div>
